    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.11.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "config": {
